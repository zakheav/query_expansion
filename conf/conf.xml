<?xml version="1.0" encoding="UTF-8"?>
<recipe>
    <worker>
        <num>2</num>
    </worker>
    <master>
        <!--ip and port of master node-->
        <ip>127.0.0.1</ip>
        <port>8080</port>

        <setSize>0,2</setSize>
        <!--frequency beyond 20 will be treated as frequent set-->
        <support>20</support>
        <iterationTimes>15</iterationTimes>
        <!--use to reject the noise, between 0.5~1.0, the bigger the factor is, the less the noise have-->
        <iterationFactor>0.9</iterationFactor>
    </master>
</recipe>
